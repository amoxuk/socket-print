{
    "name": "socket-console",
    "displayName": "socket-console",
    "description": "tcp to vscode console",
    "version": "0.0.2",
    "publisher": "amoxuk",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension.js",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Socket Print",
            "properties": {
                "socketPrint.port": {
                    "type": "string",
                    "default": 57612,
                    "description": "需要监听的端口号"
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "eslint": "^5.13.0",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    }
}